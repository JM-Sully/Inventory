<div>
  <h1>Warehouses</h1>
  <p>All warehouses are listed below. Click <%= link_to "here", new_warehouse_path %> to add a new warehouse.</p>

  <table>
    <tr>
      <th>Warehouse Name</th>
      <th>City</th>
      <th>Country</th>
    </tr>
      <% @warehouses.each do |wh| %>
      <tr>
        <td><%= link_to wh.name, wh %></td>
        <td><%= wh.city %></td>
        <td><%= wh.country %></td>
      </tr>
    <% end %>
  </table>
</div>

<p><%= link_to "Return to Homepage", root_path %></p>
